<h1>編集</h1>

<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= form_with(model: @user, local: true) do |f| %>
        <%= f.label :image, '画像' %>
        <%= f.file_field :image %>
        <%= f.label :name,"ユーザー名" %>
        <%= f.text_field :name, class: 'form-control' %>

        <%= f.label :email,"Eメール" %>
        <%= f.email_field :email, class: 'form-control' %>

        <%= f.label :occupation,"職業" %>
        <%= f.text_field :occupation, class: 'form-control' %>

        <%= f.label :specialty, "得意分野" %>
        <%= f.select :specialty,[["材料・素材","材料・素材"],["金型","金型"],["機械加工","機械加工"],["半導体","半導体"],["電子","電子"],["設計","設計"],["FA・自動化","FA・自動化"],["ロボット","ロボット"],["品質管理","品質管理"],["環境","環境"],["生産技術","生産技術"],["化学","化学"],["化学工学","化学工学"]],{} ,{include_blank: "選択してください",  class: 'form-control'}  %>
        <%= f.submit "更新する", class: "btn btn-primary" %>
        <% end %>
        