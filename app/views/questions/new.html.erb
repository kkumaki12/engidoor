<h1>質問投稿</h1>
<div class="row">

    <div class="col-md-6 col-md-offset-3">
        <%= form_with scope: :question, url: questions_path, local: true do |f| %>
        <%= f.label :title,"タイトル" %>
        <%= f.text_field :title %>

        <%= f.label :content,"内容" %>
        <%= f.text_area :content %>

        <%= f.submit "投稿する", class: "btn btn-primary" %>
        <% end %>
    </div>
</div>