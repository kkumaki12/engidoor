[
  {
    "name": "engidoor-back-container-tf",
    "image": "547789976793.dkr.ecr.ap-northeast-1.amazonaws.com/engidoor-back-tf",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "back-tf",
        "awslogs-group": "/ecs/back-tf"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 3000,
        "hostPort": 3000
      }
    ],
    "environment": [
      {
        "name": "RAILS_ENV",
        "value": "production"
      }
    ]
    
  }
]
